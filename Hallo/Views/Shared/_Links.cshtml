@using HalloDal.Models.Content

<div class="fpBox" style="margin-top: 0px; margin-bottom: 0px;">Vigtige links</div>
@foreach (FrontpageLink link in (List<FrontpageLink>)Session["FrontpageLinks"]) { 
    <div class="menupkt" onmouseover="ev_hov(this);" onmouseout="ev_out(this)" onclick="window.open('@link.NavigationUrl')">@link.Label</div>
}
<div style="border-bottom: solid thin; margin: 0px 20px 0px 20px; width: 134px;"></div>
