@using System.Data
@model DataTable




<h2>PMO-fil indhold:</h2>
<table>
    <tr>
        @foreach (DataColumn c in Model.Columns) {
            <th>@c.ColumnName</th>
        }
    </tr>
    @foreach (DataRow r in Model.Rows) { 
        <tr>
            @for (int i = 0; i < Model.Columns.Count; i++) { 
                <td>@r[i].ToString()</td>
            }
        </tr>
    }
</table>
