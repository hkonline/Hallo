<?xml version="1.0" encoding="utf-8" ?>
<Rules xmlns="Sherwood.SignOn.Client.UrlFilter" defaultDenyRedirectUrl="http://www.brunstad.org/PublicLogin.htm">
  <Rule>
    <url>kobenhavn.brunstad.local</url>
    <roles>(PMO:StronglyMapped)</roles>
    <requireSession>true</requireSession>
  </Rule>
  <Rule>
    <url>holstebro.brunstad.org</url>
    <roles>(PMO:StronglyMapped)</roles>
    <requireSession>true</requireSession>
  </Rule>
  <Rule>
    <url>localhost</url>
    <roles>(PMO:StronglyMapped)</roles>
    <requireSession>true</requireSession>
  </Rule>
  <Rule>
    <url>holstebro.azurewebsites.net</url>
    <roles>(PMO:StronglyMapped)</roles>
    <requireSession>true</requireSession>
  </Rule>
  <!-- This entry is here to ensure access for portal.brunstad.org to rss-Feed-->
  <Rule alwaysAllow="true">
    <url>.*</url>
    <users>^content\.brunstad\.org$</users>
  </Rule>
</Rules>
